{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "globalEnv": ["OPENAI_API_KEY", "PINECONE_INDEX_URL", "PINECONE_API_KEY"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "scrape": {},
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  }
}
